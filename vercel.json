{
  "version": 2,
  "name": "declaracao-irpf-api",
  "routes": [
    {
      "src": "/api/declaracao(.*)",
      "dest": "/api/api_declaracao_sheets.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/api_declaracao_sheets.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
